@inproceedings{Kanzler2018,
abstract = {We present a voxel-based rendering pipeline for large 3D line sets that employs GPU ray-casting to achieve scalable rendering including transparency and global illumination effects that cannot be achieved with GPU rasterization. Even for opaque lines we demonstrate superior rendering performance compared to GPU rasterization of lines, and when transparency is used we can interactively render large amounts of lines that are infeasible to be rendered via rasterization. To achieve this, we propose a direction-preserving encoding of lines into a regular voxel grid, along with the quantization of directions using face-to-face connectivity in this grid. On the regular grid structure, parallel GPU ray-casting is used to determine visible fragments in correct visibility order. To enable interactive rendering of global illumination effects like low-frequency shadows and ambient occlusions, illumination simulation is performed during ray-casting on a level-of-detail (LoD) line representation that considers the number of lines and their lengths per voxel. In this way we can render effects which are very difficult to render via GPU rasterization. A detailed performance and quality evaluation compares our approach to rasterization-based rendering of lines.},
archivePrefix = {arXiv},
arxivId = {1801.01155},
author = {Kanzler, Mathias and Rautenhaus, Marc and Westermann, Ruediger},
booktitle = {IEEE Transactions on Visualization and Computer Graphics},
doi = {10.1109/TVCG.2018.2834372},
eprint = {1801.01155},
issn = {10772626},
keywords = {Ray-casting,global illumination,large 3D line sets,transparency},
pages = {1--1},
title = {{A Voxel-based Rendering Pipeline for Large 3D Line Sets}},
url = {https://ieeexplore.ieee.org/document/8356687/},
year = {2018}
}
@article{Keylock2012,
abstract = {Kolmogorov�s classic theory for turbulence assumed an independence$\backslash$nbetween velocity increments and the value for the velocity itself.$\backslash$nHowever, recent work has called this assumption in to question, which$\backslash$nhas implications for the structure of atmospheric, oceanic and fluvial$\backslash$nflows. Here we propose a conceptually simple analytical framework$\backslash$nfor studying velocity-intermittency coupling that is similar in essence$\backslash$nto the popular quadrant analysis method for studying near-wall flows.$\backslash$nHowever, we study the dominant (longitudinal) velocity component$\backslash$nalong with a measure of the roughness of the signal, given mathematically$\backslash$nby its series of H�lder exponents. Thus, we permit a possible dependence$\backslash$nbetween velocity and intermittency. We compare boundary layer data$\backslash$nobtained in a wind tunnel to turbulent jets and wake flows. These$\backslash$nflow classes all have distinct characteristics, which cause them$\backslash$nto be readily distinguished using our technique and the results are$\backslash$nrobust to changes in flow Reynolds numbers. Classification of environmental$\backslash$nflows is then possible based on their similarities to the idealized$\backslash$nflow classes and we demonstrate this using laboratory data for flow$\backslash$nin a parallel-channel confluence. Our results have clear implications$\backslash$nfor sediment transport in a range of geophysical applications as$\backslash$nthey suggest that the recently proposed impulse-based methods for$\backslash$nstudying bed load transport are particularly relevant in domains$\backslash$nsuch as gravel bed river flows where the boundary layer is disrupted$\backslash$nand wake interactions predominate.},
author = {Keylock, C. J. and Nishimura, K. and Peinke, J.},
doi = {10.1029/2011JF002127},
isbn = {0148-0227},
issn = {21699011},
journal = {Journal of Geophysical Research: Earth Surface},
month = {mar},
number = {1},
pages = {n/a--n/a},
title = {{A classification scheme for turbulence based on the velocity-intermittency structure with an application to near-wall flow and with implications for bed load transport}},
url = {http://doi.wiley.com/10.1029/2011JF002127},
volume = {117},
year = {2012}
}
@article{Keylock2011,
abstract = {The specification of inlet conditions for eddy-resolving simulations is an important research question in turbulence research. A large number of schemes have been proposed but comparisons to a benchmark simulation are usually undertaken in such a way that it is ambiguous as to whether the source of the improved preservation of the properties of the benchmark case is due to the physics captured in the inlet generation algorithm or to differences in the precise values or spectra generated at a particular position. This article uses gradual wavelet reconstruction to degrade a precursor simulation such that the time series input at each cell has the same histogram and Fourier spectrum as the original precursor case, thereby removing this source of variability. A parameter, $\rho$, is used to index the nature of the degraded inlets and as $\rho$ increases from 0 to 1 the cross-correlative and nonlinear properties of the precursor are increasingly preserved too. We compare the results of four large-eddy simulations for the flow over a wall-mounted square rib and show that there are clear differences between these simulations for the flow before and over the rib, and in the far-field. However, in the wake, the intense shearing and mixing means that any differences are harder to attribute to the nature of the inlet. Hence, for flows that are mixed strongly, there is less of a need to preserve the cross-correlative structure of the inlet condition as long as the individual velocity components are modelled with an appropriate set of values and Fourier spectrum. By comparing particular inlet generation algorithms in terms of the value for $\rho$ that they can attain, their suitability for modelling flows of particular configurations may be determined.},
author = {Keylock, C. J. and Tokyay, T. E. and Constantinescu, G.},
doi = {10.1080/14685248.2011.636047},
issn = {1468-5248},
journal = {Journal of Turbulence},
month = {jan},
pages = {N45},
title = {{A method for characterising the sensitivity of turbulent flow fields to the structure of inlet turbulence}},
url = {http://www.tandfonline.com/doi/abs/10.1080/14685248.2011.636047},
volume = {12},
year = {2011}
}
