journal = {Journal of Fluid Mechanics},
keywords = {Dynamics,Isotropic,Theory},
month = {dec},
pages = {497--507},
publisher = {Cambridge University Press},
title = {{Expanding the Q-R space to three dimensions}},
url = {http://www.journals.cambridge.org/abstract{\_}S0022112009991947},
volume = {641},
year = {2009}
}
